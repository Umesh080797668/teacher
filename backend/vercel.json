{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/generate-qr.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/check-auth.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/check-env.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/check-env",
      "dest": "api/check-env.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/web-session/generate-qr",
      "dest": "api/generate-qr.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/web-session/check-auth/(.*)",
      "dest": "api/check-auth.js?sessionId=$1",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ]
}